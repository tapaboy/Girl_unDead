#: scenarios/02_escape.cfg:5
msgid "Escape"
msgstr ""

#. [side]
#: scenarios/02_escape.cfg:86
msgid "Villager"
msgstr ""

#. [side]
#: scenarios/02_escape.cfg:107
msgid "Strange Mage"
